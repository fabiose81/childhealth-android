<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".activities.vaccine.VaccineHistoryActivity">

    <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/tvNameVaccine" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="5dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="5dp"
            android:layout_marginTop="5dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="5dp"
            android:fontFamily="@font/kaeden_bold" android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent" android:gravity="center_vertical|center_horizontal"/>
</android.support.constraint.ConstraintLayout>
